{
  "storage": "../apihub-root",
  "port": 8080,
  "preventRateLimit": true,
  "activeComponents": [
    "virtualMQ",
    "messaging",
    "notifications",
    "filesManager",
    "bdns",
    "bricksLedger",
    "bricksFabric",
    "bricking",
    "anchoring",
    "dsu-wizard",
    "ctr-match-service",
    "pdm-dsu-toolkit-app-store",
    "pdm-dsu-toolkit-app-commands",
    "ctr-dsu-wizard",
    "staticServer"
  ],
  "componentsConfig": {
    "ctr-match-service": {
      "module": "./../../ctr-dsu-wizard/middleware",
      "function": "Init"
    },
    "pdm-dsu-toolkit-app-commands": {
      "module": "./../../pdm-dsu-toolkit",
      "function": "Init"
    },
    "ctr-dsu-wizard": {
      "module": "./../../ctr-dsu-wizard",
      "function": "Init"
    },
    "bricking": {},
    "anchoring": {}
  },
  "enableRequestLogger": true,
  "enableJWTAuthorisation": false,
  "enableLocalhostAuthorization": false,
  "skipJWTAuthorisation": [
    "/assets",
    "/leaflet-wallet",
    "/dsu-fabric-wallet",
    "/directory-summary",
    "/resources",
    "/bdns",
    "/anchor/epi",
    "/anchor/default",
    "/anchor/vault",
    "/bricking",
    "/bricksFabric",
    "/bricksledger",
    "/create-channel",
    "/forward-zeromq",
    "/send-message",
    "/receive-message",
    "/files",
    "/notifications",
    "/mq"
  ]
}